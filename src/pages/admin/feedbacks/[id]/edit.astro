---
import RichCard from "@/components/RichCard.astro";
import AdminLayout from "@/layouts/AdminLayout.astro";
import SaveForm from "../_parts/SaveForm";
const token = Astro.cookies.get('token')?.value as string;
const { id } = Astro.params;
---

<AdminLayout>
    <RichCard title="Edit Feedback" action={{ url: "/admin/feedbacks" }}>
        <SaveForm token={token} feedbackId={id} client:load />
    </RichCard>
</AdminLayout>