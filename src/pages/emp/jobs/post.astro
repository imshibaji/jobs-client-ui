---
import EmployeeLayout from "@/layouts/EmpLayout.astro";
import SaveForm from "./_parts/SaveForm";
import { User } from "@/utils/types/User";
const token = Astro.cookies.get('token')?.value as string;
const user = Astro.cookies.get('user')?.json() as User;
---
<EmployeeLayout>
  <SaveForm token={token} user={user} client:load />
</EmployeeLayout>